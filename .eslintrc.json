{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"airbnb-base",
		"prettier"
	],
	"plugins": ["prettier"],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"consistent-return": "off",
		"import/prefer-default-export": "off",
		"prettier/prettier": ["warn", {
			"endOfLine": "auto",
			"singleQuote": false,
			"arrowParens": "avoid",
			"bracketSpacing": true
		}]
	}
}
